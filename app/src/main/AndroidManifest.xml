<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.londonbaby">

    <application
        android:allowBackup="true"
        android:icon="@drawable/london"
        android:label="@string/app_name"
        android:roundIcon="@drawable/london"
        android:supportsRtl="true"
        android:theme="@style/Theme.LondonBaby">
        <activity
            android:name=".DetailedActivity"
            android:label="@string/title_activity_detailed"
            android:theme="@style/Theme.LondonBaby.NoActionBar"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>